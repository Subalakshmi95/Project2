@model Project2.Models.Booking

<h2>Delete Booking</h2>
<h3>@TempData</h3>

<h3>Are you sure you want to delete this booking?</h3>

<div>
    <dl class="row">
        <dt class="col-sm-2">
            Name
        </dt>
        <dd class="col-sm-10">
            @Model.Name
        </dd>
        <dt class="col-sm-2">
            Email
        </dt>
        <dd class="col-sm-10">
            @Model.Email
        </dd>
    </dl>
</div>

<form method="post">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-action="DisplayList" class="btn btn-secondary">Cancel</a>
</form>
